         ,"Settings":{
            "Language":"PE.PROPERTY.SOURCE_SNIPPET_LANG.LOV.PLSQL.D"
           ,"PL\/SQL Code":"DECLARE\nv_wybor_algorytmu NUMBER;\nBEGIN\n  IF :P10_ID_KLIENTA IS NULL OR :P10_ILOSC IS NULL OR :P10_DLUGOSC IS NULL OR :P10_SZEROKOSC IS NULL OR :P10_TERMIN_REALIZACJI IS NULL THEN\n    RAISE_APPLICATION_ERROR(-20000, \u0027Wszystkie pola musz\u0105 by\u0107 wype\u0142nione\u0027);\n  ELSE\n    dodaj_zamowienie(\n      :P10_ID_KLIENTA,\n      :P10_ILOSC,\n      :P10_DLUGOSC,\n      :P10_SZEROKOSC,\n      TO_DATE(:P10_TERMIN_REALIZACJI, \u0027YYYY-MM-DD\u0027)\n    );\n\n    v_wybor_algorytmu := APEX_UTIL.GET_SESSION_STATE(\u0027ALGORYTM_WYKORZSTUJACY_POLE\u0027);\n    uruchom_sekwencje_ciec(v_wybor_algorytmu, 10); \n  END IF;\nEND;"
