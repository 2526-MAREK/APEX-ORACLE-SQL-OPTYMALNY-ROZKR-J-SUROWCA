      "id":"19694172581427151642"
     ,"Identification":{
        "Name":"dodanie_zamowienia"
      }
     ,"Execution":{
        "Sequence":10
       ,"Event Scope":"Static"
       ,"Type":"Immediate"
      }
     ,"When":{
        "Event":"EVENT.EVENT.BROWSER.CLICK"
       ,"Selection Type":"Button"
       ,"Button":"Dodaj_zamowienie_do_klienta (19694172487832151641)"
      }
     ,"Actions":[
        {
          "id":"19694172662182151643"
         ,"Identification":{
            "Action":"Execute Server-side Code"
          }
         ,"Settings":{
            "Language":"PE.PROPERTY.SOURCE_SNIPPET_LANG.LOV.PLSQL.D"
           ,"PL\/SQL Code":"DECLARE\n    v_id_klienta_session VARCHAR2(100);\n    v_wybor_algorytmu NUMBER;\nBEGIN\nIF  :P15_ILOSC IS NULL OR :P15_DLUGOSC IS NULL OR :P15_SZEROKOSC IS NULL OR :P15_TERMIN_REALIZACJI IS NULL THEN\n    RAISE_APPLICATION_ERROR(-20000, \u0027Wszystkie pola musz\u0105 by\u0107 wype\u0142nione. Warto\u015B\u0107 P2_ID_KLIENTA: \u0027 || NVL(:P2_ID_KLIENTA, \u0027NULL\u0027));\n  ELSE\n\n    v_id_klienta_session := APEX_UTIL.GET_SESSION_STATE(\u0027P1_ID_KLIENTA\u0027);\n    v_wybor_algorytmu := APEX_UTIL.GET_SESSION_STATE(\u0027ALGORYTM_WYKORZSTUJACY_POLE\u0027);\n\n    DBMS_OUTPUT.PUT_LINE(\u0027P1_ID_KLIENTA\u0027 || v_id_klienta_session);\n\n    dodaj_zamowienie(\n      v_id_klienta_session,\n      :P15_ILOSC,\n      :P15_DLUGOSC,\n      :P15_SZEROKOSC,\n      TO_DATE(:P15_TERMIN_REALIZACJI, \u0027YYYY-MM-DD\u0027)\n    );\n    --pierwszy argument wybor algorytmu , drugi ilosc zamowien w sekwencji \n    -- numer 1 algorytmu to jest r\u00F3znica p\u00F3l\n    -- numer 2 algorytmu to jest r\u00F3\u017Cnica bok\u00F3w \n    v_wybor_algorytmu := APEX_UTIL.GET_SESSION_STATE(\u0027ALGORYTM_WYKORZSTUJACY_POLE\u0027);\n    uruchom_sekwencje_ciec(2, 1); \n    END IF;\nEND;"
           ,"Items to Submit":[
              "P2_ID_KLIENTA"
             ,"P15_ILOSC"
             ,"P15_DLUGOSC"
             ,"P15_SZEROKOSC"
             ,"P15_TERMIN_REALIZACJI"
            ]
          }
         ,"Execution":{
            "Sequence":10
           ,"Event":"dodanie_zamowienia (19694172581427151642)"
           ,"Fire When Event Result Is":"True"
           ,"Fire on Initialization":false
           ,"Stop Execution On Error":true
           ,"Wait For Result":true
          }
        }
      ]
    }
